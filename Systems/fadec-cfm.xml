	<filter>
		<name>Throttle 1</name>
		<debug>false</debug>
		<type>gain</type>
		<gain>1</gain>
		<enable>
			<condition>
				<and>
					<equals>
						<property>/systems/thrust/alpha-floor</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/systems/thrust/toga-lk</property>
						<value>0</value>
					</equals>
					<or>
						<equals>
							<property>/systems/thrust/state1</property>
							<value>MAN THR</value>
						</equals>
						<equals>
							<property>/systems/thrust/state1</property>
							<value>CL</value>
						</equals>
						<equals>
