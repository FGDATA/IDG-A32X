						<cos>
							<property>attitude/roll-rad</property>
						</cos>
					</quotient>
					<value>1</value>
				</sum>
			</function>
		</fcs_function>
		
		<fcs_function name="fbw/pitch/g-error">
			<function>
				<difference>
